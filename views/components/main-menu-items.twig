{% for item in main_menu.items %}

<li class="{{ item.classes|join(' ') }}">
    <a class="hover:font-bold" href="{{ item.link }}" {{ item.is_target_blank ? 'target="_blank"' }}>{{item.title }}</a>
</li>

{% endfor %}

<!-- Contact modal button -->
<li>
    <a x-data @click.prevent="$dispatch('open-modal-contact'); console.log('Event sent')" 
       class="hover:font-bold" href="#">
        Contact
    </a>
</li>
